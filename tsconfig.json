{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["ESNext"],
    "types": ["node", "vite/client"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
